[binaries]
c = 'gcc'
cpp = 'g++'
ar = 'ar'
strip = 'strip'
pkg-config = 'pkg-config'
rust = 'rustc'   # or 'rustc -m32' if you use a wrapper script (better: just 'rustc')

[host_machine]
system = 'linux'
cpu_family = 'x86'
cpu = 'i386'
endian = 'little'

[built-in options]
prefix = '/usr'
libdir = '/usr/lib32'
includedir = '/usr/include'

[properties]
needs_exe_wrapper = true
